generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("coklogi_PRISMA_URL") // uses connection pooling
  directUrl = env("coklogi_URL_NON_POOLING") // uses a direct connection
}
model Post {
  id        String   @id @default(cuid())
  name      String   @db.VarChar(100)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
